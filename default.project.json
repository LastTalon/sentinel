{
	"name": "Sentinel",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json", "**/*.spec.lua"],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"sentinel": {
				"$path": "out/src/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"sentinel": {
				"$path": "out/src/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"sentinel": {
					"$path": "out/src/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$ignoreUnknownInstances": true,
			"$properties": {
				"FilteringEnabled": true
			},
			"Level": {
				"$path": "assets/level.rbxm"
			},
			"Terrain": {
				"$path": "assets/terrain.rbxm"
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
